{
    "builds": [
        {
            "src": "powergym_vercel.py",
            "use": "@vercel/python"
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/powergym_vercel.py"
        }
    ]
}